{
  "name": "@moeru/std-workspace",
  "type": "module",
  "version": "0.1.0-beta.8",
  "private": true,
  "packageManager": "pnpm@10.15.0",
  "scripts": {
    "build": "pnpm -r build",
    "build:packages": "pnpm -rF \"./packages/*\" build",
    "bump": "bumpp",
    "dev": "pnpm -rF @moeru/std-docs dev",
    "install-cli": "pnpm -rF @moeru/eslint-config build && pnpm i",
    "lint": "moeru-lint --flag unstable_native_nodejs_ts_config",
    "test": "pnpm -r test"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.2.1",
    "@importantimport/tsconfig": "1.0.0-beta.2",
    "@moeru/eslint-config": "workspace:^",
    "@moeru/tsconfig": "workspace:^",
    "@types/node": "^24.3.0",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "bumpp": "^10.2.3",
    "eslint": "^9.34.0",
    "eslint-plugin-oxlint": "^1.13.0",
    "jiti": "^2.5.1",
    "oxlint": "^1.13.0",
    "pkgroll": "^2.15.3",
    "tsx": "^4.20.5",
    "vitest": "^3.2.4"
  }
}
