    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "bumpp": "^10.1.1",
    "eslint": "^9.24.0",
    "eslint-plugin-oxlint": "^1.3.0",
    "jiti": "^2.4.2",
